        {
            "taskName": "conan-install",
            "appliesTo": "conanfile.txt",
            "type": "launch",
            "contextType": "build",
            "command": "conan",
            "args": [ "install", "." ],
            "envVars": {
                "CONAN_PRINT_RUN_COMMANDS": "1"
            }
        }
    ]
}
